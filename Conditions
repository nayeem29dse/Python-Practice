#Write a Python program to find maximum between two numbers using if else
x = int(input("Input first num: "))
y = int(input("Input second num: "))

if x > y:
    print(f"{x} is maximum.")
else:
    print(f"{y} is maximum.")

#Write a Python program to find maximum between three numbers using ladder if else or nested if
x = int(input("Input first num: "))
y = int(input("Input second num: "))
z = int(input("Input third num: "))
if x>y and x>z:
    print(f"{x} is maximum.")
elif y>x and y>z:
    print(f"{y} is maximum.")
else:
    print(f"{z} is maximum.")
#Alternative
x = int(input("Input first num: "))
y = int(input("Input second num: "))
z = int(input("Input third num: "))
if y>x>z:
    print(f"{x} is maximum.")
elif x>y>z:
    print(f"{y} is maximum.")
else:
    print(f"{z} is maximum.")

#Alternative
x = int(input("Input first num: "))
y = int(input("Input second num: "))
z = int(input("Input third num: "))
if x>y:
    if x>z:
        print(f"{x} is maximum.")
    else:
        print(f"{z} is maximum.")
else:
    if y>z:
        print(f"{y} is maximum.")
    else:
        print(f"{z} is maximum.")

#Write a Python program to check positive, negative or zero using simple if or if else.
x = int(input("Input a num: "))
if x>0:
    print(x, "is a positive Number")
elif x<0:
    print(x, "is a negative Number")
else:
    print(x, "is zero")


#Write a Python program to check whether a number is divisible by 5 and 11 or not using if else.
x = int(input("Input a num: "))
if x%5==0 and x%11==0:
    print(x, "is divisable by 5 and 11")
elif x%5==0:
    print(x, "is divisable by 5")
elif x%11==0:
    print(x, "is divisable by 11")
else:
    print(x, "is not divisable by either 5 or 11")

#Write a C program to check leap year using if else.
x = int(input("Input a year: "))
if x%4==0 and x%100==0 or x%400==0:
    print(x, "is a leap year")
else:
    print(x,'is not aleap year')


